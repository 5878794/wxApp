

.title{
  width:100%; height: 80rpx; line-height: 80rpx;
  font-size:36rpx; color: #000; font-weight: bold;
  text-align: center;
  padding:0 40rpx;
  margin-top:40rpx;
}


.address{
  width: 100%; font-size: 24rpx; color: #007aff;
  text-align: center; line-height: 40rpx;
  margin: 0 0 20rpx 0;
  padding:0 40rpx;
}

.images{
  width: 100%; height: 300rpx; margin-top: 40rpx;
}
.images image{
  width: 300rpx; height: 300rpx; display: block;
}


.texts{
  font-size:24rpx; color: #333; line-height: 40rpx;
  text-indent: 2em;
  width:90%;
  margin:0 auto;
  margin-top:40rpx;
  box-sizing: border-box;

}


.btn{
  width: 80%; height: 80rpx; line-height: 80rpx;
  text-align: center; color: #fff;
  background: dodgerblue;
  border-radius: 40rpx;
  margin: 0 auto;
  margin-top: 80rpx;
}

.hasGo{
  background: #eee;
}
