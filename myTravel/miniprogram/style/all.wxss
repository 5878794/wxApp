.box_slt > view,
.box_slc > view,
.box_slb > view,
.box_sct > view,
.box_scc > view,
.box_scb > view,
.box_srt > view,
.box_src > view,
.box_srb > view,
.box_hlt > view,
.box_hlc > view,
.box_hlb > view,
.box_hct > view,
.box_hcc > view,
.box_hcb > view,
.box_hrt > view,
.box_hrc > view,
.box_hrb > view{
    flex:0 0 auto;
}

.canTap{
    transition: all 0.2s linear;
}
.canTap:active{
    opacity:0.5;
}

.animate{
    -webkit-transition: all 0.2s linear;
    -moz-transition: all 0.2s linear;
    -ms-transition: all 0.2s linear;
    -o-transition: all 0.2s linear;
    transition: all 0.2s linear;
}
.animate1{
    -webkit-transition: all 0.4s ease-out;
    -moz-transition: all 0.4s ease-out;
    -ms-transition: all 0.4s ease-out;
    -o-transition: all 0.4s ease-out;
    transition: all 0.2s ease-out;
}

.canActive{
    -webkit-transition: all 0.2s linear;
    -moz-transition: all 0.2s linear;
    -ms-transition: all 0.2s linear;
    -o-transition: all 0.2s linear;
    transition: all 0.2s linear;
}
.canActive:active{
    opacity:0.6;
}


.hidden{
    display: none;
